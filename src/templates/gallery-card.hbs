{{#each this}}
  <div class='photo-card gallery__item'>
    <a class='link' href='{{largeImageURL}}'>
      <div class='gallery-item__wrapper'>
        <div class='gallery-img-thumb'>
          <img
            src='{{webformatURL}}'
            alt='{{tags}}'
            loading='lazy'
            class='gallery-img'
            width="250px"
          /></div>
        <div class='info'>
          <p class='info-item'>
            <b>Likes</b>
            {{likes}}
          </p>
          <p class='info-item'>
            <b>Views</b>
            {{views}}
          </p>
          <p class='info-item'>
            <b>Comments</b>
            {{comments}}
          </p>
          <p class='info-item'>
            <b>Downloads</b>
            {{downloads}}
          </p>
        </div>
      </div>
    </a>
  </div>
{{/each}}

